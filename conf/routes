# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index()
GET        /init                controllers.Application.initDrone()
GET        /battery             controllers.Application.getBatteryPercentage()
GET        /location            controllers.Application.getLocation()
GET        /rotation            controllers.Application.getRotation()
GET        /speed               controllers.Application.getSpeed()
GET        /altitude            controllers.Application.getAltitude()

#GET        /takeoff             controllers.Application.takeOff()
GET        /land                controllers.Application.land()

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)
